<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">




	<changeSet author="a.arab" id="dropTable-employee">
		<dropTable cascadeConstraints="true" tableName="employee" />
	</changeSet>

	<changeSet author="a.arab" id="dropTable-department">
		<dropTable cascadeConstraints="true" tableName="department" />

	</changeSet>

	<changeSet author="a.arab" id="dropTable-department2">
		<dropTable cascadeConstraints="true" tableName="department2" />
	</changeSet>

	<changeSet author="a.arab" id="dropTable-databasechangelog">
		<dropTable cascadeConstraints="true" tableName="databasechangelog" />
	</changeSet>

	<changeSet author="a.arab" id="dropTable-databasechangeloglock">
		<dropTable cascadeConstraints="true" tableName="databasechangeloglock" />
	</changeSet>
	
	<changeSet id="tag-2.0" author="a.arab">
		<tagDatabase tag="2.0" />
	</changeSet>

</databaseChangeLog>